{
  "name": "smc-desktopreact",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint ./frontend/src --ext .ts,.tsx",
    "lint:fix": "eslint ./frontend/src --ext .ts,.tsx --fix",
    "format": "prettier --write \"frontend/src/**/*.{js,ts,jsx,tsx,json,css}\"",
    "format:check": "prettier --check \"frontend/src/**/*.{js,ts,jsx,tsx,json,css}\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "typescript": "^4.9.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5"
  }
}
